{
  "name": "@spring-media/red-sourcepoint-cmp",
  "version": "0.8.0",
  "description": "Sourcepoint CMP Library for www.bild.de",
  "typings": "dist/types.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "postbuild": "npm run build-html-snippets && npm run build-cae-package && cp src/types.d.ts dist/types.d.ts",
    "lint": "eslint --ext .js,.ts .",
    "preplayground": "npm run build",
    "playground": "node ./scripts/prepare-playground && serve ./playground",
    "build-html-snippets": "node ./scripts/build-html-snippets.js",
    "storybook": "start-storybook -p 6006 -s playground/assets",
    "build-cae-package": "node ./scripts/build-cae-package.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./tcf-v2": "./dist/esm/tcf-v2.js",
    "./callbacks": "./dist/esm/callbacks.js",
    "./vuex-module": "./dist/esm/vuex-module.js"
  },
  "keywords": [
    "sourcepoint",
    "consent-management",
    "tcf-v2.0",
    "consent-management-platform",
    "transparency-and-consent-framework",
    "spring-media"
  ],
