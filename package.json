{
  "name": "@spring-media/red-sourcepoint-cmp",
  "version": "0.20.1",
  "description": "Sourcepoint CMP Library for www.bild.de",
  "scripts": {
    "test": "jest --config tests/jest.config.js",
    "build": "rollup -c",
    "postbuild": "npm run build-cae-package",
    "lint": "eslint --ext .js,.ts,.vue .",
    "preplayground": "npm run build",
    "playground": "node ./scripts/prepare-playground && serve ./playground",
    "storybook": "start-storybook -p 9001 -s playground/assets",
    "storybook:static": "serve .storybook-static",
    "storybook:build": "build-storybook --quiet -c .storybook -o .storybook-static && cp -R playground/assets/* .storybook-static",
    "build-cae-package": "node ./scripts/build-cae-package.js",
    "preimage-snapshots": "docker run -p 9222:3000 --name chrome --rm -d browserless/chrome:1.32.0-chrome-stable",
    "image-snapshots": "jest --config tests/jest.image-snapshots.config.js tests/image-snapshots.runner.js",
    "postimage-snapshots": "docker stop chrome || exit 0",
    "image-snapshots:ci": "CI=true jest --config tests/jest.image-snapshots.config.js tests/image-snapshots.runner.js",
    "check-cae-bundle": "es-check es5 .cae/*.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "sourcepoint",
    "consent-management",
    "tcf-v2.0",
    "consent-management-platform",
    "transparency-and-consent-framework",
    "spring-media"
  ],
  "author": {
    "name": "Mario Bartlack",
    "url": "https://spring-media.de"
  },
  "bugs": {
    "url": "https://github.com/spring-media/red-sourcepoint-cmp/issues"
  },
  "homepage": "https://github.com/spring-media/red-sourcepoint-cmp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/spring-media/red-sourcepoint-cmp.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-react": "7.9.4",
    "@spring-media/storybook-addon-image-snapshots": "0.10.3",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/vue": "5.3.18",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "@vue/eslint-config-typescript": "5.0.2",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-jest": "25.3.0",
    "babel-loader": "8.1.0",
    "babel-preset-vue": "2.0.2",
    "clean-css": "4.2.3",
    "es-check": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-vue": "6.2.2",
    "fs-extra": "9.0.0",
    "jest": "25.3.0",
    "jest-transform-stub": "2.0.0",
    "prettier": "2.0.4",
    "rollup": "2.6.1",
    "rollup-plugin-postcss": "2.6.4",
    "rollup-plugin-typescript2": "0.27.0",
    "rollup-plugin-vue": "5.1.6",
    "serve": "11.3.0",
    "terser": "4.6.11",
    "ts-jest": "25.3.1",
    "ts-loader": "7.0.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3",
    "vue": "2.6.11",
    "vue-jest": "3.0.5",
    "vue-loader": "15.9.1",
    "vue-server-renderer": "2.6.11",
    "vue-template-compiler": "2.6.11",
    "vuex": "3.1.3"
  },
  "peerDependencies": {
    "vue": "2.6.11",
    "vuex": "3.1.3"
  },
  "dependencies": {}
}
